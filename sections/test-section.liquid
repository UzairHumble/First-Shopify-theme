{% comment %} {{ section.settings.custom_liquid }}
{% schema %}
{
   "name": "Gold Chart Live",
   "settings": [
   {
      "type": "liquid",
      "id": "custom_liquid",
      "label": "Gold Chart",
      "info": "Add app snippets or other liquid code to create advanced customizations."
   }],
   "presets": [
   {
      "name": "Gold Chart"
   }]
}
{% endschema %} {% endcomment %}


